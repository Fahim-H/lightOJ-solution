#include <iostream>
using namespace std;
int order=1;
int main()
{
    int looper,sum,till_my_floor;
    cin>>looper;
    while(looper--){
        long long int x1,y1,x2,y2,x,y;
        int m;
        cin>>x1>>y1>>x2>>y2;
        cin.ignore();
        cin>>m;
        cout<<"Case "<<order<<":"<<endl;
        for(int i=0;i<m;i++){
            cin>>x>>y;
            if(x>=x1 && x<=x2 && y>=y1 && y<=y2){
                cout<<"Yes"<<endl;
            }
            else{
                cout<<"No"<<endl;
            }
        }
        order+=1;
    }
    return 0;
}
