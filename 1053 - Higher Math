#include <iostream>
using namespace std;
int order=1;
int main()
{
    int looper;
    cin>>looper;
    while(looper--){
        int len1,len2,len3;
        cin>>len1>>len2>>len3;
        if((len1*len1)==(len2*len2)+(len3*len3) ||
            (len2*len2)==(len1*len1)+(len3*len3) ||
            (len3*len3)==(len1*len1)+(len2*len2)) {
            cout<<"Case "<<order<<": yes"<<endl;
        }
        else{
            cout<<"Case "<<order<<": no"<<endl;
        }
        order=order+1;
    }
    return 0;
}
